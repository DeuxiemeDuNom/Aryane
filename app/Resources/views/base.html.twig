{# app/Resources/views/layout.html.twig #}
{# Chemin absolu : /thaliemero/web/bundles/tmblog/ #}
<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <meta name="keywords" content="Développeur, web, Symfony™, démonstration, couture, bijoux, porcelaine, aryane" />
  <meta name="author" content="Martial Etoka">
  <meta name="copyright" content="©  Martial Etoka." />
  <meta name="description" content="Site de démonstration." />
  <title>{% block title %}Aryane{% endblock %}</title>

  {% stylesheets 
      'bundles/tmblog/css/*'
       filter='cssrewrite'
       output="css/compiled/main.css"
  %}
    <!-- Google Fonts -->
    <link href='http://fonts.googleapis.com/css?family=Titillium+Web:400,200,300,700,600' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Roboto+Condensed:400,700,300' rel='stylesheet' type='text/css'>
    <link href='http://fonts.googleapis.com/css?family=Raleway:400,100' rel='stylesheet' type='text/css'>
    <link href='https://fonts.googleapis.com/css?family=Michroma' rel='stylesheet' type='text/css'>

    <!-- Bootstrap -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/css/bootstrap.min.css">
    
    <!-- Font Awesome -->
    <link rel="stylesheet" href="http://maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css">

    <!-- Custom CSS -->
    <link rel="stylesheet" href="{{ asset_url }}" type="text/css" />

    <!-- Favicon -->
    <link rel="icon" type="image/png" href="{{ asset('bundles/tmblog/img/aryafav.png') }}">
  {% endstylesheets %}

    <!-- Mary Lou Menu -->
    <script src="/thaliemero/web/bundles/tmblog/js/modernizr.custom.js"></script>

</head>
<body>

  {# Block BODY #}
  {% block body %}
  {% endblock body %}

  {# ================================================ Scripts ================================================ #}

     <!-- jQuery -->
    <script src="/thaliemero/web/bundles/tmblog/js/jquery.min.js"></script>

    <!-- Mary Lou -->
    <script src="/thaliemero/web/bundles/tmblog/js/jquery.dlmenu.js"></script>

    <!-- Dropdown -->
    <script src="/thaliemero/web/bundles/tmblog/js/dropdown.js"></script>

    <!-- Bootstrap JS form CDN -->
    <script src="http://maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
    
    <!-- jQuery sticky menu -->
    <script src="/thaliemero/web/bundles/tmblog/js/owl.carousel.min.js"></script>
    <script src="/thaliemero/web/bundles/tmblog/js/jquery.sticky.js"></script>
    
    <!-- jQuery easing -->
    <script src="/thaliemero/web/bundles/tmblog/js/jquery.easing.1.3.min.js"></script>

    <!-- Main Script -->
    <script src="/thaliemero/web/bundles/tmblog/js/main.js"></script>
    <script>
        $(function() {
            $( '#dl-menu' ).dlmenu({
                animationClasses : { classin : 'dl-animate-in-3', classout : 'dl-animate-out-3' }
            });
        });
    </script>
</body>
</html>